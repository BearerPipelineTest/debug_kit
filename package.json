    "license": "MIT",
    "homepage": "https://github.com/cakephp/debug_kit#readme",
    "bugs": {
        "url": "https://github.com/cakephp/debug_kit/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cakephp/debug_kit.git"
